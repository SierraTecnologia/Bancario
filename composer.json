{
    "name": "sierratecnologia/bancario",
    "description": "bancario",
    "type": "library",
    "keywords": [
        "sierratecnologia",
        "bancario"
    ],
    "license": "MIT",
    "homepage": "https://sierratecnologia.com/packages/bancario/",
    "support": {
        "email": "help@sierratecnologia.com.br",
        "issues": "https://github.com/sierratecnologia/bancario/issues",
        "source": "https://github.com/sierratecnologia/bancario",
        "docs": "https://github.com/sierratecnologia/bancario/blob/master/README.md"
    },
    "authors": [
        {
            "name": "SierraTecnologia",
            "homepage": "https://sierratecnologia.com.br",
            "email": "help@sierratecnologia.com.br"
        },
        {
            "name": "Ricardo Rebello Sierra",
            "email": "contato@ricardosierra.com.br"
        }
    ],
    "require": {
        "php": ">=7.0",
        "sierratecnologia/muleta": "dev-master",
        "sierratecnologia/crypto": "dev-master",
        "sierratecnologia/payment-laravel": "dev-master"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories",
            ".discovery"
        ],
        "psr-4": {
            "Bancario\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "repositories": [
        {
            "type": "path",
            "url": "/sierra/Dev/Libs/**",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "vcs",
            "url":  "git@github.com:botman/studio-addons.git"
        }
    ],
    "extra": {
        "laravel": {
            "dont-discover": [
                "sentry/sentry-laravel",
                "laravel/cashier",
                "hyn/multi-tenant",
                "scaffold-interface"
            ]
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
